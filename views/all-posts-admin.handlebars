<div class="posts list-group">
  {{#each posts}}
  <a class="list-item flex-row justify-space-between" href="/dashboard/edit/{{id}}">
    <span>{{title}}</span>
    <span>{{format_date created_at}}</span>
  </a>
  {{/each}}
  {{#if logged_in}}
  <script src="/js/logout.js"></script>
  {{/if}}
</div>